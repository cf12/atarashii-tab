{
  "name": "Atarashii (New) Tab Page",
  "description": "Displays anime wallpapers from r/Animewallpaper on your new tab page.",
  "version": "0.1.3",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "https://i.redd.it/"
  ]
}
